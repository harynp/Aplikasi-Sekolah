<!DOCTYPE html>
<html>
<%- include ./partials/session.ejs %>
  <h1><small>SCHOOLS</small></h1>
  <h2>Pages Students</h2>
  <body>
    <div class="container-fluid">
      <table class="table table-stripped">
        <thead>
          <tr>
            <th>ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Action</th>
            <th colspan="2">Options</th>
          </tr>
        </thead>
        <tbody>
        <%dataStudents.forEach(row=>{ %>
          <tr>
            <td><%= row.id%></td>
            <td><%= row.first_name%></td>
            <td><%= row.last_name%></td>
            <td><%= row.email%></td>
            <td><a href="/students/addsubjects/<%= row.id%>">Add Subject</td>
            <td><a href="/students/edit/<%= row.id%>">Edit</td>
            <td><a href="/students/delete/<%= row.id%>">Delete</td>
          </tr>
        <%})%>
        </tbody>
      </table>
      <a href="/students/add"><button type="button" name="add">Add</button></a>
      <a href="/index"><button type="button" name="back">Back</button></a>
    </div>
  </body>
</html>
